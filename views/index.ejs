<body>


  <div class="container-contact100">
    <div class="wrap-contact100">
      <form class="contact100-form validate-form" action="">
        <span class="contact100-form-title" style="text-transform: uppercase;">
          Get in Touch with SkyDecor
        </span>

        <div class="wrap-input100 validate-input" data-validate="Name is required">
          <span class="label-input100">Full Name</span>
          <input class="input100" type="text" name="name" placeholder="John Doe">
          <span class="focus-input100"></span>
        </div>

        <div class="wrap-input100 validate-input" data-validate="Valid email is required: ex@abc.xyz">
          <span class="label-input100">Email</span>
          <input class="input100" type="text" name="email" placeholder="Enter your email addess">
          <span class="focus-input100"></span>
        </div>

        <div class="wrap-input100 validate-input" data-validate="Valid mobile number is required">
          <span class="label-input100">Mobile Number</span>
          <div style="display: flex; gap: 0.5rem;">
            <select class="input100" name="country_code" required style="flex: 0.5; border: none;">
              <option value="+1">ðŸ‡ºðŸ‡¸ +1 (USA)</option>
              <option value="+91">ðŸ‡®ðŸ‡³ +91 (India)</option>
              <option value="+44">ðŸ‡¬ðŸ‡§ +44 (UK)</option>
              <option value="+61">ðŸ‡¦ðŸ‡º +61 (Australia)</option>
              <option value="+81">ðŸ‡¯ðŸ‡µ +81 (Japan)</option>
              <option value="+49">ðŸ‡©ðŸ‡ª +49 (Germany)</option>
              <option value="+971">ðŸ‡¦ðŸ‡ª +971 (UAE)</option>
              <option value="+86">ðŸ‡¨ðŸ‡³ +86 (China)</option>
              <!-- Add more country codes as needed -->
            </select>
            <input class="input100" type="tel" name="mobile_number" placeholder="9876543210" required style="flex: 1;">
          </div>
          <span class="focus-input100"></span>
        </div>

        <div class="wrap-input100 input100-select">
          <span class="label-input100 ">User Type</span>
          <div class="mt-2">
            <select class="selection-2" name="service" style="border: none; width: 100%;">
              <option>Architect</option>
              <option>Tech Partner</option>
              <option>Trade Partner</option>
            </select>
          </div>
          <span class="focus-input100"></span>
        </div>


        <div class="wrap-input100 validate-input" data-validate="Name is required">
          <span class="label-input100">Company Name </span>
          <input class="input100" type="text" name="name" placeholder="Registered business name">
          <span class="focus-input100"></span>
        </div>

        <div class="wrap-input100 validate-input" data-validate="Message is required">
          <span class="label-input100">Company Address </span>
          <textarea class="input100" name="address"
            placeholder="Full address including street, city, state, and ZIP/postal code"></textarea>
          <span class="focus-input100"></span>
        </div>

        <div class="container-contact100-form-btn">
          <div class="wrap-contact100-form-btn">
            <div class="contact100-form-bgbtn"></div>
            <button class="contact100-form-btn">
              <span>
                Submit
                <i class="fa fa-long-arrow-right m-l-7" aria-hidden="true"></i>
              </span>
            </button>
          </div>
        </div>
      </form>
    </div>
  </div>



  <script>
    $(".selection-2").select2({
      minimumResultsForSearch: 20,
      dropdownParent: $('#dropDownSelect1')
    });
  </script>


